{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "deja",
  "main": "src/index.ts",
  "compatibility_date": "2024-01-01",
  "workers_dev": true,
  "durable_objects": {
    "bindings": [
      { "name": "DEJA", "class_name": "DejaDO" }
    ]
  },
  "migrations": [
    { "tag": "v1", "new_sqlite_classes": ["DejaDO"] }
  ],
  "vectorize": [
    { "binding": "VECTORIZE", "index_name": "deja-embeddings" }
  ],
  "ai": {
    "binding": "AI"
  },
  "triggers": {
    "crons": ["0 0 * * *"]
  }
}
